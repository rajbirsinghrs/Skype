<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a1691586.skype.skype">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="576dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        android:weightSum="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="378dp"
            android:layout_height="54dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <TextView

                android:id="@+id/textView"
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:paddingLeft="12dp"
                android:paddingTop="15dp"
                android:text="Edit"
                android:textColor="#00aff0"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.46"
                android:paddingLeft="50dp"
                android:paddingTop="12dp"
                app:srcCompat="@drawable/abc" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="53dp"
                android:layout_height="match_parent"
                android:paddingRight="30dp"
                android:paddingTop="15dp"
                app:srcCompat="@drawable/twoo" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="33dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:layout_weight="0.18"
                app:srcCompat="@drawable/onee" />

        </LinearLayout>

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="352dp"
            android:layout_height="27dp"
            android:layout_marginLeft="6dp"
            android:layout_marginTop="8dp"
            android:paddingTop="5dp"
            app:srcCompat="@drawable/search"
            android:layout_weight="0.02" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:layout_marginTop="10dp"
            android:layout_weight="0.09"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="@android:dimen/notification_large_icon_height"
                android:layout_height="match_parent"
                android:layout_weight="0.05"
                app:srcCompat="@drawable/eeee" />

            <LinearLayout
                android:layout_width="173dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="6dp"
                    android:text="Amonika"
                    android:textColor="@android:color/black" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="149dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Ayi ni pic" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="0.83"
                android:text="01-17"
                android:textAlignment="textEnd" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:layout_marginTop="375dp"
            android:orientation="horizontal"
            android:layout_weight="0.04">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.14"
                app:srcCompat="@drawable/drecent" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/contact"
                android:layout_weight="0.09" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/calls" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.12"
                app:srcCompat="@drawable/dmy" />

        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
